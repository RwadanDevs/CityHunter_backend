{
  "name": "CityHunter_backend",
  "version": "1.0.0",
  "description": "GPS Tracker",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./server/index.js",
    "test": "set NODE_ENV=test && npm run devtables && nyc --reporter=html --reporter=text mocha --require @babel/register ./server/tests/*.js --recursive ./server/tests/*.js --exit",
    "devtables": "npm run droptables && npm run newtables && npm run seeds",
    "dev": "babel-watch ./server/index.js",
    "seeds": "npx sequelize-cli db:seed:all",
    "newtables": "npx sequelize-cli db:migrate",
    "droptables": "npx sequelize-cli db:migrate:undo:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RwadanDevs/CityHunter_backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RwadanDevs/CityHunter_backend/issues"
  },
  "homepage": "https://github.com/RwadanDevs/CityHunter_backend#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@hapi/joi": "^17.1.1",
    "babel-eslint": "^10.1.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0",
    "bcrypt": "^4.0.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
